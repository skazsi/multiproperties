<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="com.skzs.multiproperties.feature"
      label="MultiProperties Feature"
      version="2.0.0">

   <description>
      The MultiProperties Editor help for editing several properties
files having similar structure in one table.
   </description>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="com.skzs.multiproperties.base"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.forms"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.jface.text"/>
   </requires>

   <plugin
         id="com.skzs.multiproperties.base"
         download-size="0"
         install-size="0"
         version="2.0.0"
         unpack="false"/>

   <plugin
         id="com.skzs.multiproperties.ui"
         download-size="0"
         install-size="0"
         version="2.0.0"
         unpack="false"/>

</feature>
